<?xml version="1.0" encoding="UTF-8" ?>
<XMLDB PATH="question/type/ltjprocessed/db" VERSION="20060825" COMMENT="XMLDB file for this question type">
  <TABLES>
    <TABLE NAME="question_ltjprocessed_servers" COMMENT="Servers that the Remote Processed Question will use" NEXT="question_ltjprocessed">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" ENUM="false" COMMENT="id of the table" NEXT="servername"/>
	<FIELD NAME="servername" TYPE="char" LENGTH="255" NOTNULL="true" SEQUENCE="false" ENUM="false" COMMENT="Display name of the server" PREVIOUS="id" NEXT="serverurl"/>
	<FIELD NAME="serverurl" TYPE="char" LENGTH="511" NOTNULL="true" SEQUENCE="false" ENUM="false" COMMENT="URL of server" PREVIOUS="servername"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id" COMMENT="primary key of the table"/>	
      </KEYS>
    </TABLE>
    <TABLE NAME="question_ltjprocessed" COMMENT="Question templates for Remote Processed Questions" PREVIOUS="question_ltjprocessed_servers" NEXT="question_ltjprocessed_answers">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" ENUM="false" NEXT="question"/>
        <FIELD NAME="question" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" ENUM="false" PREVIOUS="id" NEXT="serverid"/>
        <FIELD NAME="serverid" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" ENUM="false" PREVIOUS="question" NEXT="variables"/>
        <FIELD NAME="variables" TYPE="text" NOTNULL="false" UNSIGNED="true" SEQUENCE="false" ENUM="false" PREVIOUS="serverid"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id" COMMENT="primary key of the table" NEXT="question"/>	
        <KEY NAME="question" TYPE="foreign" FIELDS="question" REFTABLE="questions" REFFIELDS="id" PREVIOUS="primary" NEXT="serverid"/> 
        <KEY NAME="serverid" TYPE="foreign" FIELDS="serverid" REFTABLE="question_ltjprocessed_servers" REFFIELDS="id" PREVIOUS="question"/> 
      </KEYS>
    </TABLE>
    <TABLE NAME="question_ltjprocessed_answers" COMMENT="Extra fields for answers for ltjprocessed questions" PREVIOUS="question_ltjprocessed" NEXT="question_ltjprocessed_states">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" ENUM="false" COMMENT="id of the table" NEXT="question"/>
        <FIELD NAME="question" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" ENUM="false" PREVIOUS="id" NEXT="answer"/>
        <FIELD NAME="answer" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" ENUM="false" PREVIOUS="question" NEXT="tolerance"/>
        <FIELD NAME="tolerance" TYPE="char" LENGTH="255" NOTNULL="true" DEFAULT="0.0" SEQUENCE="false" ENUM="false" PREVIOUS="answer" NEXT="remotegrade"/>
        <FIELD NAME="remotegrade" TYPE="int" LENGTH="1" NOTNULL="true" DEFAULT="0" SEQUENCE="false" ENUM="false" PREVIOUS="tolerance"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id" COMMENT="primary key of the table" NEXT="question"/>	
        <KEY NAME="question" TYPE="foreign" FIELDS="question" REFTABLE="questions" REFFIELDS="id" PREVIOUS="primary" NEXT="answer"/> 
        <KEY NAME="answer" TYPE="foreign" FIELDS="answer" REFTABLE="question_answers" REFFIELDS="id" PREVIOUS="question"/> 
      </KEYS>
    </TABLE>
    <TABLE NAME="question_ltjprocessed_states" COMMENT="Stored states" PREVIOUS="question_ltjprocessed_answers" NEXT="question_ltjprocessed_ans_states">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" ENUM="false" COMMENT="id of the table" NEXT="question"/>
        <FIELD NAME="question" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" ENUM="false" COMMENT="id of the table" PREVIOUS="id" NEXT="attempt"/>
        <FIELD NAME="attempt" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" ENUM="false" COMMENT="id of the table" PREVIOUS="question" NEXT="state"/>
        <FIELD NAME="state" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" ENUM="false" COMMENT="id of the table" PREVIOUS="attempt" NEXT="questiontext"/>
        <FIELD NAME="questiontext" TYPE="text" NOTNULL="true" UNSIGNED="true" ENUM="false" COMMENT="id of the table" PREVIOUS="state" NEXT="workspace"/>
        <FIELD NAME="workspace" TYPE="char" LENGTH="255" NOTNULL="true" UNSIGNED="true" ENUM="false" COMMENT="id of the table" PREVIOUS="questiontext"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id" COMMENT="primary key of the table" NEXT="question"/>	
        <KEY NAME="question" TYPE="foreign" FIELDS="question" REFTABLE="questions" REFFIELDS="id" PREVIOUS="primary" NEXT="attempt"/> 
        <KEY NAME="attempt" TYPE="foreign" FIELDS="attempt" REFTABLE="question_attempts" REFFIELDS="id" PREVIOUS="question" NEXT="state"/> 
        <KEY NAME="state" TYPE="foreign" FIELDS="state" REFTABLE="question_states" REFFIELDS="id" PREVIOUS="attempt"/> 
      </KEYS>
    </TABLE>
    <TABLE NAME="question_ltjprocessed_ans_states" COMMENT="Stored states" PREVIOUS="question_ltjprocessed_states">
      <FIELDS>
        <FIELD NAME="id" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="true" ENUM="false" COMMENT="id of the table" NEXT="answer"/>
        <FIELD NAME="answer" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" SEQUENCE="false" ENUM="false" PREVIOUS="id" NEXT="question"/>
        <FIELD NAME="question" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" ENUM="false" COMMENT="id of the table" PREVIOUS="answer" NEXT="attempt"/>
        <FIELD NAME="attempt" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" ENUM="false" COMMENT="id of the table" PREVIOUS="question" NEXT="state"/>
        <FIELD NAME="state" TYPE="int" LENGTH="10" NOTNULL="true" UNSIGNED="true" ENUM="false" COMMENT="id of the table" PREVIOUS="attempt" NEXT="answertext"/>
        <FIELD NAME="answertext" TYPE="text" NOTNULL="true" SEQUENCE="false" ENUM="false" PREVIOUS="state" NEXT="tolerance"/>
        <FIELD NAME="tolerance" TYPE="char" LENGTH="255" NOTNULL="true" DEFAULT="0.0" SEQUENCE="false" ENUM="false" PREVIOUS="answertext"/>
      </FIELDS>
      <KEYS>
        <KEY NAME="primary" TYPE="primary" FIELDS="id" COMMENT="primary key of the table" NEXT="question"/>	
        <KEY NAME="question" TYPE="foreign" FIELDS="question" REFTABLE="questions" REFFIELDS="id" PREVIOUS="primary" NEXT="attempt"/> 
        <KEY NAME="attempt" TYPE="foreign" FIELDS="attempt" REFTABLE="question_attempts" REFFIELDS="id" PREVIOUS="question" NEXT="state"/> 
        <KEY NAME="state" TYPE="foreign" FIELDS="state" REFTABLE="question_states" REFFIELDS="id" PREVIOUS="attempt" NEXT="answer"/> 
        <KEY NAME="answer" TYPE="foreign" FIELDS="answer" REFTABLE="question_answers" REFFIELDS="id" PREVIOUS="state"/> 
      </KEYS>
    </TABLE>
  </TABLES>
</XMLDB>
